{
    "name":"default",
    "bundleid": "com.example.myapp",
    "ignore_modules": [
        "@edgeros/eslint-plugin-jsre",
        "@edgeros/jsre-types"
    ],
    "ignore_path": [
        "./test/**",
        "./temp/**"
    ],
    "assets": {
        "ico_big": "assets/big.png",
        "ico_small": "assets/small.png"
    },
    "program": {
        "gss": false,
        "log": "console",
        "will": false,
        "reside": false,
        "mesv": "1.0.0",
        "experimental": false
    },
    "loading":{
		"splash":"$assets.splash",
		"background":"#000000",
		"animation":"enlarge"
	},
    "update": {
        "remove": [
            "public"
        ]
    },
    "widget": [
        {
            "ico": "$assets.widget_icon",
            "name": "My Widget",
            "path": "widget",
            "rows": 2,
            "columns": 2,
            "category": "Demo"
        }
    ],
    "vendor": {
        "id": "000000",
        "name": "mycompany",
        "email": "example@example.com",
        "phone": "123456789",
        "fax": ""
    }
}